# Multi-Bot Forum Application Environment Variables
# ==================================================
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env to version control!

# Reddit Bot 1 Credentials
REDDIT_BOT1_CLIENT_ID=your_client_id_here
REDDIT_BOT1_CLIENT_SECRET=your_client_secret_here
REDDIT_BOT1_USERNAME=your_username_here
REDDIT_BOT1_PASSWORD=your_password_here

# Reddit Bot 2 Credentials
REDDIT_BOT2_CLIENT_ID=your_client_id_here
REDDIT_BOT2_CLIENT_SECRET=your_client_secret_here
REDDIT_BOT2_USERNAME=your_username_here
REDDIT_BOT2_PASSWORD=your_password_here

# Reddit Bot 3 Credentials
REDDIT_BOT3_CLIENT_ID=your_client_id_here
REDDIT_BOT3_CLIENT_SECRET=your_client_secret_here
REDDIT_BOT3_USERNAME=your_username_here
REDDIT_BOT3_PASSWORD=your_password_here

# OpenAI API Key (optional, for AI-powered content generation)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (optional, for AI-powered content generation)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Encryption Key (generate with: python -c "import secrets; print(secrets.token_urlsafe(32))")
DB_ENCRYPTION_KEY=your_encryption_key_here

# Proxy Settings (optional)
HTTP_PROXY=
HTTPS_PROXY=

# Application Settings
APP_ENV=production
LOG_LEVEL=INFO
